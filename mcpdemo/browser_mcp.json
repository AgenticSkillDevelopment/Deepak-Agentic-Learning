{
    "mcpServers": {
      "playwright": {
        "command": "npx",
        "args": [
          "@playwright/mcp@latest"
        ]
      }
    },
    "airbnb": {
      "command": "npx",
      "args": [
        "-y",
        "@openbnb/mcp-server-airbnb"
      ]
    },
    "duckduckgo-search": {
      "command": "npx",
      "args": [
        "-y",
        "duckduckgo-mcp-server"
      ]
    },
    "arxiv-mcp-server": {
          "command": "uv",
          "args": [
              "tool",
              "run",
              "arxiv-mcp-server",
              "--storage-path", "/home/Deepak/mcpdemo/paper/storage"
          ]
      }
}